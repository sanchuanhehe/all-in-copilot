{
	"name": "minimax-copilot",
	"publisher": "sanchuan",
	"displayName": "MiniMax Copilot",
	"description": "MiniMax provider for VS Code Chat",
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/sanchuanhehe/all-in-copilot"
	},
	"version": "1.0.13",
	"type": "module",
	"engines": {
		"vscode": "^1.104.0"
	},
	"categories": [
		"AI",
		"Chat"
	],
	"keywords": [
		"minimax",
		"MiniMax",
		"copilot",
		"chat",
		"ai",
		"llm",
		"language model"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run package",
		"download-api": "dts dev",
		"postdownload-api": "dts main",
		"postinstall": "npm run download-api",
		"compile": "node esbuild.js",
		"watch": "node esbuild.js --watch",
		"package": "node esbuild.js --production",
		"vsce:package": "vsce package --no-dependencies",
		"lint": "eslint",
		"format": "prettier --write .",
		"test": "pnpm run compile && vscode-test"
	},
	"dependencies": {
		"@all-in-copilot/sdk": "workspace:*"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "^22",
		"@vscode/dts": "^0.4.1",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "^3.7.1",
		"esbuild": "^0.24.2",
		"eslint": "^9.13.0",
		"prettier": "^3.1.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0"
	},
	"contributes": {
		"languageModelChatProviders": [
			{
				"vendor": "minimax",
				"displayName": "MiniMax",
				"managementCommand": "minimax.manage"
			}
		],
		"commands": [
			{
				"command": "minimax.manage",
				"title": "Configure MiniMax"
			}
		]
	}
}
